{
  "name": "preliquify",
  "private": false,
  "license": "MIT",
  "author": "Preliquify",
  "description": "Build Shopify Liquid snippets using React/Preact components with TypeScript/JSX",
  "homepage": "https://github.com/MichaelNusair/preliquify#readme",
  "keywords": [
    "shopify",
    "liquid",
    "react",
    "preact",
    "jsx",
    "typescript",
    "compiler",
    "shopify-theme",
    "shopify-development",
    "template-engine"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MichaelNusair/preliquify"
  },
  "bugs": {
    "url": "https://github.com/MichaelNusair/preliquify/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "version": "1.1.5",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "pnpm -r --filter '@preliquify/*' run build",
    "dev": "pnpm --filter @preliquify/cli run dev",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "publish-all-packages": "node scripts/publish.mjs"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@testing-library/preact": "^3.2.4",
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^9.12.0",
    "globals": "^15.9.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vitest": "^2.0.5"
  }
}
