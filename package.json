{
  "name": "preliquify",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "pnpm -r --filter '@preliquify/*' run build",
    "dev": "pnpm --filter @preliquify/cli run dev",
    "lint": "eslint .",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "publish": "pnpm -r --filter '@preliquify/*' run publish"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "typescript": "^5.6.3",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3"
  }
}
