{
  "name": "preliquify",
  "private": false,
  "license": "MIT",
  "author": "Preliquify",
  "repository": {
    "type": "git",
    "url": "https://github.com/MichaelNusair/preliquify"
  },
  "bugs": {
    "url": "https://github.com/MichaelNusair/preliquify/issues"
  },
  "version": "0.1.36",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "pnpm -r --filter '@preliquify/*' run build",
    "dev": "pnpm --filter @preliquify/cli run dev",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "publish-all-packages": "node scripts/publish.mjs"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@vitest/coverage-v8": "^2.0.5",
    "typescript": "^5.6.3",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "vitest": "^2.0.5",
    "jsdom": "^24.1.0",
    "@testing-library/preact": "^3.2.4"
  }
}
