{
  "name": "preliquify",
  "private": false,
  "license": "MIT",
  "author": "Preliquify",
  "repository": {
    "type": "git",
    "url": "https://github.com/preliquify/preliquify"
  },
  "homepage": "https://preliquify.com",
  "bugs": {
    "url": "https://github.com/preliquify/preliquify/issues"
  },
  "version": "0.1.1",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "pnpm -r --filter '@preliquify/*' run build",
    "dev": "pnpm --filter @preliquify/cli run dev",
    "lint": "eslint .",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "publish:packages": "pnpm -r --filter '@preliquify/*' publish",
    "publish": "pnpm build && pnpm publish:packages"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "typescript": "^5.6.3",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3"
  }
}
